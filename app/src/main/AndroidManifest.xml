<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.melihozden.mobileprogrammingodev">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".KullaniciActivity" android:label="Settings Screen"></activity>
        <activity android:name=".SensorActivity" android:label="Sensors Screen" />
        <activity android:name=".KullaniciListesiActivity" android:label="Users List" />
        <activity android:name=".emailActivity" android:label="Email Screen" />
        <activity android:name=".MenuActivity" android:label="Menu Screen"/>
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>